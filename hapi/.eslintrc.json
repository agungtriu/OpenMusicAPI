{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": 0,
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": "off",
    "no-console": "off",
    "no-unused-vars": "off"
  }
}
